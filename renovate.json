{
  "extends": [
    "github>defenseunicorns/uds-common//config/renovate.json5",
    ":semanticCommits",
    ":semanticCommitTypeAll(chore)"
  ],
  "ignorePresets": [
    ":ignoreModulesAndTests"
  ],
  "branchConcurrentLimit": 0,
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "matchManagers": [
        "npm"
      ],
      "matchFileNames": [
        "package-lock.json"
      ],
      "groupName": "All npm dependencies",
      "groupSlug": "npm-dependencies"
    }
  ]
}
